class Firstcode
{
	public static void main(String args[])
	{
		int x=3,n=2;
		int ans=pow(x,n);		
		System.out.println(ans);
	}
	
	public static int pow(int x,int n)
	{
		if(n==0)
		{
			return 1;
		}
		if(x==0)
		{
			return 0;
		}
		int xpownm1= pow(x,n-1);
		int xpow=xpownm1*x;
		return xpow;
	}
}
