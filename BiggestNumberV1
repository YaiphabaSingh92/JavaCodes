
public class project1 {

	public static void main(String[] args) {
		{
    //Program to find biggest number version1
		int x1=3;
		int x2=-4;	
		int x3=-8;
		if ((x1==x2 && x1==x3) || (x1==x2 && x1>x3)  || (x1==x3 && x1>x2) || (x2==x1 && x2>x3) || (x2==x3 && x2>x1) ) {System.out.println("Equals");}
		else if (x1>x2 && x1>x3) {System.out.println(x1);}
		else if (x2>x1 && x2>x3) {System.out.println(x2);}
		else if (x3>x1 && x3>x2) {System.out.println(x3);}
	}

}
}
