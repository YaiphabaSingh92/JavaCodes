import java.util.*;
class Firstcode
{
	public static void main(String args[])
	{
		int i,n=0,j;
		for(i=1;i<=5;i++)
		{
			
			
			for(j=1;j<=i;j++)
			{
				if((j+i)%2==0)
				{
					System.out.print("1");
				}
				else
				{
					System.out.print("0");
				}
			}
			System.out.println(" ");
		}
	}	
}
