 class Firstcode
 {
	 public static void main(String args[])
	 {
		 int arr[]= {6,3,7,4,2,5};
		 
		 for(int i=0;i<arr.length-1;i++)
		 {
			 int small=i;
			 for(int j=i+1;j<arr.length;j++)
			 {
				 if(arr[small]>arr[j])
				 {
					 small=j; //index of smallest number
				 }
			 }
			 int temp=arr[i];
			 arr[i]=arr[small];
			 arr[small]=temp;
			 
		 }
		 
		 for(int i=0;i<arr.length;i++)
		 {
			 System.out.print(arr[i]+" ");
		 }
	 }
	 
	 
 }
